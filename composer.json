{
    "name": "elegancecms/dev-tool",
    "description": "Dev tool commands for Elegance CMS",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "elegancecms",
        "elegancecms cms",
        "elegancecms platform",
        "elegancecms dev tools"
    ],
    "homepage": "https://elegancecms.com",
    "authors": [
        {
            "name": "EleganceCMS Technologies",
            "email": "contact@elegancecms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EleganceCMS\\DevTool\\": "src"
        }
    },
    "require": {
        "laravel/framework": "^10.31|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EleganceCMS\\DevTool\\Providers\\DevToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
